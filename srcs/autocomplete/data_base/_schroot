-S (-h --help)-h[help] (-h --help)--help[help] (-a --all)-a[select all chroots and active sessions] (-a --all)--all[select all chroots and active sessions] --all-chroots[select all chroots] --all-sessions[select all active sessions] *-c[use specified chroot]:chroot:->chroot *--chroot=[use specified chroot]:chroot:->chroot (-d --directory)-d[directory to use]:dir:_files -W / -P / (-d --directory)--directory=[directory to use]:dir:_files -W / -P / (-u --user)-u[username (default current user)]:user:_users (-u --user)--user=[username (default current user)]:user:_users (-l --list)-l[list available chroots] (-l --list)--list[list available chroots] (-i --info)-i[show information about selected chroots] (-i --info)--info[show information about selected chroots] --location[print location of selected chroots] --config[dump configuration of selected chroots] (-p --preserve-environment)-p[preserve user environment] (-p --preserve-environment)--preserve-environment[preserve user environment] (-q --quiet)-q[quiet] (-q --quiet)--quiet[quiet] (-v --verbose)-v[verbose] (-v --verbose)--verbose[verbose] (-V --version)-V[version] (-V --version)--version[version] (-b --begin-session)-b[begin a session; returns a session ID] (-b --begin-session)--begin-session[begin a session; returns a session ID] --recover-session[recover an existing session] (-e --end-session)-e[end an existing session] (-e --end-session)--end-session[end an existing session] (-f --force)-f[force operation] (-f --force)--force[force operation] (-):command name: _command_names -e *::arguments: _normal
