-d[ftp verbosity]
--debug[ftp verbosity]
-f[upload ignoring logs]
--force[upload ignoring logs]
-k[keep going, skip broken]
--keep[keep going, skip broken]
--no[dry run]
--nomail[suppress announcement]
--mailonly[dry run and real announcement]
--noarchive[add anti-archival header]
-p[print config legibly]
--print[print config legibly]
-q[quiet]
--quiet[quiet]
-V[display version information]
--Version[display version information]
