(-q --query)-q[be quiet and make the output more parseable] (-q --query)--query[be quiet and make the output more parseable] (-r --registry-dir)-r[set the registry directory] (-r --registry-dir)--registry-dir[set the registry directory] (-s --source-dir)-s[set addon source directory] (-s --source-dir)--source-dir[set addon source directory] (-t --target-dir -w --system-wide)-t[set addon target directory] (-t --target-dir -w --system-wide)--target-dir[set addon target directory] (-v --verbose -z --silent)-v[increase verbosity] (-v --verbose -z --silent)--verbose[increase verbosity] (y --system-dir)-y[set system-wide target directory] (y --system-dir)--system-dir[set system-wide target directory] (-)-h[show help information] (-)--help[show help information] (-w --system-wide -t --target-dir)-w[use system-wide target directory] (-w --system-wide -t --target-dir)--system-wide[use system-wide target directory] (-z --silent -v --verbose)-z[supress most output] (-z --silent -v --verbose)--silent[supress most output] 1:commands:((
    install\:install\ specified\ addons
    remove\:remove\ specified\ addons
    list\:list\ available\ addons\ in\ registry
    status\:list\ status\ of\ addons
    disable\:disable\ specified\ addons
    enable\:undo\ effects\ of\ previous\ disable
    files\:list\ files\ composing\ the\ specified\ addon
    show\:display\ detailed\ information\ on\ specified\ addon
  )) *: :->args
