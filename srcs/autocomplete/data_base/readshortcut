-f[show field names]
--fields[show field names]
-u[use Unix path format for display (default)]
--unix[use Unix path format for display (default)]
-w[use Windows path format for display ]
--windows[use Windows path format for display ]
-t[display shortcut target]
--target[display shortcut target]
-g[display shortcut working directory]
--working[display shortcut working directory]
-r[display shortcut arguments]
--args[display shortcut arguments]
-s[display shortcut "show" command value]
--showcmd[display shortcut "show" command value]
-i[display icon library location]
--icon[display icon library location]
-j[display icon library offset]
--offset[display icon library offset]
-d[display shortcut description]
--desc[display shortcut description]
-a[display all information]
--all[display all information]
-h[display help information]
--help[display help information]
--usage[display brief usage message]
-v[display version information]
--version[display version information]
--license[display licensing information]
