-A[define widget alias]
-C[define completion widget]
-D[delete widget]
-L[with -l, list as commands]
-a[with -l, list all widgets]
-F[install file descriptor handler]
-M[display message]
-N[define new widget]
-c[with -R, clear listing]
-U[unget to input stack]
-T[manipulate transformations]
-r[remove transformations]
