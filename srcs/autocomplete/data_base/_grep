-S -s (-e --regexp -f --file)1: :_guard "^--*" pattern *:files:_files (--extended-regexp --fixed-strings --basic-regexp --perl-regexp -E -F -G -P)-E[use extended regular expression] (--extended-regexp --fixed-strings --basic-regexp --perl-regexp -E -F -G -P)-F[use literal strings] (--extended-regexp --fixed-strings --basic-regexp --perl-regexp -E -F -G -P)-G[use basic regular expression] (--extended-regexp --fixed-strings --basic-regexp --perl-regexp -E -F -G -P)-P[use perl regular expression] (--after-context -A)-A+[specify lines of trailing context]:lines (--text -a --binary-files -I)-a[process binary file as if it were text] (--before-context -B)-B+[specify lines of leading context]:lines (--context,-C)-C-[specify lines of context]:lines (--byte-offset -b -c)-b[print the byte offset with output lines] (--count -c --byte-offset -b --line-number -n)-c[only print a count of matching lines] (--directories -d -r --recursive)-d+[specify handling of directories]:action on directory:(read skip recurse) (--devices -D)-D+[specify handling of devices, FIFOs and sockets]:action on device:(read skip) (1)*-e+[specify pattern]:pattern (1)*-f+[specify pattern file]:file:_files (--with-filename -H --no-filename -h)-H[print filename with each match] (--no-filename -h --with-filename -H --null -Z --files-without-match -L --file-with-matches -l)-h[suppress printing of filenames] (--text -a --binary-files)-I[process binary files as if non-matching] (--ignore-case -i -y)-i[case-insensitive] (--ignore-case -i -y)-y[case-insensitive] (--files-without-match -L --file-with-matches -l --no-filename -h)-L[output non-matching files' names only] (--files-with-matches -l --files-without-match -L --no-filename -h)-l[output matching files' names only] (--max-count -m)-m+[stop after specified no of matches]:max number of matches (--line-number -n -c)-n[prefix output with line numbers] (--only-matching -o)-o[show only matching part of line] (--quiet --silent -q)-q[suppress normal output] (--recursive -r -R -d --directories)-r[recurse subdirectories] (--recursive -r -R -d --directories)-R[recurse subdirectories] *--include=[examine files matching specified pattern]:file pattern *--exclude=[skip files matching specified pattern]:file pattern *--exclude-from=[skip files matching pattern in specified file]:file:_files *--exclude-dir=[skip directories matching specified pattern]:directory pattern (--no-messages -s)-s[suppress messages about unreadable] (--version -V)-V[display version info] (--invert-match -v)-v[select non-matching lines] (--word-regexp -w --line-regexp -x)-w[force pattern to match only whole words] (--line-regexp -x --word-regexp -w)-x[force pattern to match only whole lines] (--null -Z --no-filename -h)-Z[print 0 byte after FILE name]
