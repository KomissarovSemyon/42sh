--version[show version]
--help[display help]
-h[display help]
--dump[dump configuration]
--trace[print extended information]
--verbose[verbose mode]
-v[verbose mode]
--quiet[silent mode]
-q[silent mode]
--delete[delete messages from server]
-d[delete messages from server]
-l[don't delete messages from server]
--all[retrieve all messages]
-a[retrieve all messages]
--new[retrieve only new messages]
-n[retrieve only new messages]
