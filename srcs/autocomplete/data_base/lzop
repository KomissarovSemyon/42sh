-c[write on standard output]
--stdout[write on standard output]
-d[decompress]
--decompress[decompress]
--uncompress[decompress]
-x[extract]
--extract[extract]
-f[force overwrite]
--force[force overwrite]
-h[display help information]
-H[display help information]
--help[display help information]
-l[list compressed file contents]
--list[list compressed file contents]
-L[display software license]
--license[display software license]
-n[do not save or restore the original name and time stamp]
-N[save or restore the original name and time stamp]
--name[save or restore the original name and time stamp]
-q[suppress all warnings]
--quiet[suppress all warnings]
--silent[suppress all warnings]
-r[operate recursively on directories]
--recursive[operate recursively on directories]
-t[test compressed file integrity]
--test[test compressed file integrity]
-v[verbose mode]
--verbose[verbose mode]
-V[display version number]
--version[display version number]
--info[list internal header fields for each compressed file]
-I[display information about the system]
--sysinfo[display information about the system]
-o[specify output file]
-p[write output files to specified directory]
-F[do not store or verify a checksum]
-k[keep (don't delete) input files]
--keep[keep (don't delete) input files]
-U[delete input files]
--unlink[delete input files]
--delete[delete input files]
--mono[assume a mono ANSI terminal]
--color[assume a color ANSI terminal]
--intro[display intro sequence]
