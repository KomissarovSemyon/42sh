-a[list all instances]
-d[list dependencies]
-D[list dependents]
-H[suppress header line]
-l[print detailed status about services and instances]
-o[display specific columns]
-p[list processes]
-R[list services with the given restarter]
-s[sort by a column]
-S[reverse sort by a column]
-v[verbose columns]
-x[display explanation for service states]
