-s -S : (-a --auth)-a[rewrite anonymous checkout URLs to authenticated ones] (-a --auth)--auth[rewrite anonymous checkout URLs to authenticated ones] (-p --print -d --details)-d[print detailed information; do not checkout] (-p --print -d --details)--details[print detailed information; do not checkout] (-h --help)-h[show usage message] (-h --help)--help[show usage message] (-p --print -d --details)-p[print summary information; do not checkout] (-p --print -d --details)--print[print summary information; do not checkout] (-P --package)-P+[specify package name explicitly (when target is a URL)]:package name:_deb_packages available (-P --package)--package=[specify package name explicitly (when target is a URL)]:package name:_deb_packages available (-t --type)-t+[specify repository type]:repository type:((arch\:Arch bzr\:Bazaar cvs\:CVS darcs\:Darcs git\:Git hg\:Mercurial svn\:Subversion)) (-t --type)--type=[specify repository type]:repository type:((arch\:Arch bzr\:Bazaar cvs\:CVS darcs\:Darcs git\:Git hg\:Mercurial svn\:Subversion)) (-u --username)-u+[specify username (implies --auth)]:username:  (-u --username)--username=[specify username (implies --auth)]:username:  *-f+[extract named file]:remote filename *--file=[extract named file]:remote filename --source=:policy for .orig.tar.gz file:(never auto download-only always) --git-track=[specify branches to track]:branches (separated by spaces) or '*' 1:package or URL: _alternative "_deb_packages available" "_urls" 2::destination directory:_path_files -/
