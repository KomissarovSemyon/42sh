(-h --help)-h[show summary of options] (-h --help)--help[show summary of options] (-v --verbose)-v[be more verbose] (-v --verbose)--verbose[be more verbose] (-q --quiet)-q[be more quiet] (-q --quiet)--quiet[be more quiet] (-a --arch)-a:architecture:(alpha amd64 common cygwin-i386 gnu hppa i386 linux m32r m68k mips powerpc sh sh3 sh3eb sh4 sh4eb w32) (-a --arch)--arch:architecture:(alpha amd64 common cygwin-i386 gnu hppa i386 linux m32r m68k mips powerpc sh sh3 sh3eb sh4 sh4eb w32) (-i --install)-i[install packages] (-i --install)--install[install packages] (-A --convert-anyway)-A[convert package even if not useful] (-A --convert-anyway)--convert-anyway[convert package even if not useful] (-b --build)-b[just build but do not install] (-b --build)--build[just build but do not install] (-r --remove)-r[remove packages] (-r --remove)--remove[remove packages] (-s --status)-s[print status of named packages] (-s --status)--status[print status of named packages] (-l --list)-l[print short status of named packages or patterns] (-l --list)--list[print short status of named packages or patterns] (-L --list-files)-L[list files belonging to named packages] (-L --list-files)--list-files[list files belonging to named packages] (-u --update)-u[update current cross-inst] (-u --update)--update[update current cross-inst] (-Q --query)-Q[print available update packages] (-Q --query)--query[print available update packages] *:package or path or pattern:_files
