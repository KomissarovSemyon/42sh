-C -s (-t -c -m -a)-U[don't expand aliases] (-t -M)-R[mark as read] (-t -R)-M[mark as mapped] (-t -c -m -a)*-k[ksh-style autoloading] (-t -c -m -a)*-z[zsh-style autoloading] (-t -U -z -k)-c[currently defined functions] (-t -U -z -k)-m[use names as patterns] (-t -U -z -k)-a[write autoload functions] (-M -R -U -z -k -a -c -m)-t[show table of contents] (-M -R -U -a -c -m -t):file:->file *:function:->function
