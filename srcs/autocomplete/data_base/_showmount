(-a --all)-a[list both hostname and mounted dir in host:dir format] (-a --all)--all[list both hostname and mounted dir in host:dir format] (-d --directories)-d[list only the directories mounted by some client] (-d --directories)--directories[list only the directories mounted by some client] (-e --exports)-e[show server export list] (-e --exports)--exports[show server export list] (-h --help)-h[help] (-h --help)--help[help] (-v --version)-v[version] (-v --version)--version[version] --no-headers[suppress descriptive headers from output] :host:_hosts
