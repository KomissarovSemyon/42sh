-s -S (--fields -f)-f[show field names] (--fields -f)--fields[show field names] (--unix -u --windows -w)-u[use Unix path format for display (default)] (--unix -u --windows -w)--unix[use Unix path format for display (default)] (--unix -u --windows -w)-w[use Windows path format for display ] (--unix -u --windows -w)--windows[use Windows path format for display ] (--target -t)-t[display shortcut target] (--target -t)--target[display shortcut target] (--working -g)-g[display shortcut working directory] (--working -g)--working[display shortcut working directory] (--args -r)-r[display shortcut arguments] (--args -r)--args[display shortcut arguments] (--showcmd -s)-s[display shortcut "show" command value] (--showcmd -s)--showcmd[display shortcut "show" command value] (--icon -i)-i[display icon library location] (--icon -i)--icon[display icon library location] (--offset -j)-j[display icon library offset] (--offset -j)--offset[display icon library offset] (--desc -d)-d[display shortcut description] (--desc -d)--desc[display shortcut description] (--all -a)-a[display all information] (--all -a)--all[display all information] (- 1)-h[display help information] (- 1)--help[display help information] (- 1)--usage[display brief usage message] (- 1)-v[display version information] (- 1)--version[display version information] (- 1)--license[display licensing information] 1:shortcut:_files -g "*.lnk(-.)"
