-S (-h --help)-h[help] (-h --help)--help[help] (-a --all)-a[select all chroots] (-a --all)--all[select all chroots] *-c[use specified chroot]:chroot:->chroot *--chroot=[use specified chroot]:chroot:->chroot (-d --directory)-d[directory to use]:dir:_files -W / -P / (-d --directory)--directory=[directory to use]:dir:_files -W / -P / (-l --list)-l[list available chroots] (-l --list)--list[list available chroots] (-i --info)-i[show information about selected chroots] (-i --info)--info[show information about selected chroots] (-p --listpaths)-p[print paths to available chroots] (-p --listpaths)--listpaths[print paths to available chroots] --config[dump configuration of selected chroots] (-q --quiet)-q[quiet] (-q --quiet)--quiet[quiet] (-v --verbose)-v[verbose] (-v --verbose)--verbose[verbose] (-V --version)-V[version] (-V --version)--version[version] (-):command name: _command_names -e *::arguments: _normal
