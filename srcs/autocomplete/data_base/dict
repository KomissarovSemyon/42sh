--match[perform matching]
-m[perform matching]
--nocorrect[disable spelling correction]
-C[disable spelling correction]
--dbs[list available databases]
-D[list available databases]
--strats[list available search strategies]
-S[list available search strategies]
--serverhelp[query server for help info it provides]
-H[query server for help info it provides]
--serverinfo[display info about server]
-I[display info about server]
--noauth[disable authentication]
-a[disable authentication]
--version[display program version]
-V[display program version]
--license[display program license]
-L[display program license]
--help[display usage info]
--verbose[verbose output]
-v[verbose output]
--raw[be very verbose]
-r[be very verbose]
