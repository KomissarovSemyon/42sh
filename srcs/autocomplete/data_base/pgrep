-P[parent process id]
-F[match only in process in pidfile]
-g[match only in process group ids]
-G[match only real group id]
-t[match only controlled by terminal]
-u[match only effective user id]
-U[match only real user id]
-o[oldest process]
-n[newest process]
-a[include process ancestors in the match list]
-f[match against full command line]
-i[ignore case distinctions]
-L[given pidfile must be locked]
-q[do not write anything to standard output]
-v[negate matching]
-x[match exactly]
