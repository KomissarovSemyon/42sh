-a[show all tables]
--all[show all tables]
-h[show file header]
-l[show program headers]
--segments[show program headers]
-S[show sections header]
--sections[show sections header]
-s[show symbol table]
--syms[show symbol table]
--symbols[show symbol table]
-n[show core notes (if present)]
--notes[show core notes (if present)]
-r[show relocations (if present)]
--relocs[show relocations (if present)]
-d[show dynamic section (if present)]
--dynamic[show dynamic section (if present)]
-V[show version sections (if present)]
-A[show architecture specific information (if any)]
-c[show symbol/file index in an archive]
-x[dump contents of specified section as bytes]
-p[dump contents of specified section as strings]
-I[show histogram of bucket list lengths]
--histogram[show histogram of bucket list lengths]
-W[allow output width to exceed 80 characters]
--wide[allow output width to exceed 80 characters]
-H[display help information]
--help[display help information]
-v[display version information]
--version[display version information]
