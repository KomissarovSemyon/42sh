-c[check units are reducible]
--check[check units are reducible]
-o[specify output format]
-f[specify file with units]
--file[specify file with units]
-m[- is subtraction]
--minus[- is subtraction]
-p[binary - is product]
--product[binary - is product]
-q[suppress prompts and statistics]
--quiet[suppress prompts and statistics]
--silent[suppress prompts and statistics]
-s[suppress conversion to reciprocals units]
--strict[suppress conversion to reciprocals units]
-t[make conversion output briefer]
--terse[make conversion output briefer]
-v[make output more verbose]
--verbose[make output more verbose]
-h[show help information and exit]
--help[show help information and exit]
-V[show version information and exit]
--version[show version information and exit]
