-t[terse output]
--terse[terse output]
-p[pretty output]
--pretty[pretty output]
-m[output mode]
--mode[output mode]
-f[specify fields to output]
--fields[specify fields to output]
-e[escape columns separators in values]
--escape[escape columns separators in values]
-v[show program version]
--version[show program version]
-h[print this help]
--help[print this help]
