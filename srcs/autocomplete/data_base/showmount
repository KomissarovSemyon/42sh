-a[list both hostname and mounted dir in host:dir format]
--all[list both hostname and mounted dir in host:dir format]
-d[list only the directories mounted by some client]
--directories[list only the directories mounted by some client]
-e[show server export list]
--exports[show server export list]
-h[help]
--help[help]
-v[version]
--version[version]
