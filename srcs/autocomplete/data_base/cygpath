-d[print DOS (short) form of specified path (C:\PROGRA~1\)]
--dos[print DOS (short) form of specified path (C:\PROGRA~1\)]
-m[like --windows, but with regular slashes (C:/WINNT)]
--mixed[like --windows, but with regular slashes (C:/WINNT)]
-M[report on mode of file (binmode or textmode)]
--mode[report on mode of file (binmode or textmode)]
-u[(default) print Unix form of specified path (/cygdrive/c/winnt)]
--unix[(default) print Unix form of specified path (/cygdrive/c/winnt)]
-w[print Windows form of specified path (C:\WINNT)]
--windows[print Windows form of specified path (C:\WINNT)]
-t[print type]
--type[print type]
-a[output absolute path]
--absolute[output absolute path]
-l[print Windows long form of specified path (with -w, -m only)]
-p[specified path is a PATH list (i.e., '/bin:/usr/bin')]
--path[specified path is a PATH list (i.e., '/bin:/usr/bin')]
-s[print DOS (short) form of specified path (with -w, -m only)]
-A[use 'All Users' instead of current user for -D, -P]
--allusers[use 'All Users' instead of current user for -D, -P]
-D[output 'Desktop' directory and exit]
--desktop[output 'Desktop' directory and exit]
-H[output 'Profiles' directory (home root) and exit]
--homeroot[output 'Profiles' directory (home root) and exit]
-P[output Start Menu 'Programs' directory and exit]
--smprograms[output Start Menu 'Programs' directory and exit]
-S[output system directory and exit]
--sysdir[output system directory and exit]
-W[output 'Windows' directory and exit]
--windir[output 'Windows' directory and exit]
-f[read FILE for input; use - to read from STDIN]
--file[read FILE for input; use - to read from STDIN]
-o[read options from FILE as well (for use with --file)]
--option[read options from FILE as well (for use with --file)]
-c[close specified handle (for use in captured process)]
--close[close specified handle (for use in captured process)]
-i[ignore missing argument]
--ignore[ignore missing argument]
-h[display usage information]
--help[display usage information]
-v[display version information]
--version[display version information]
