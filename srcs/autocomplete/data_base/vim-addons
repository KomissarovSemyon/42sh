-q[be quiet and make the output more parseable]
--query[be quiet and make the output more parseable]
-r[set the registry directory]
-s[set addon source directory]
-t[set addon target directory]
-v[increase verbosity]
--verbose[increase verbosity]
-y[set system-wide target directory]
-h[show help information]
--help[show help information]
-w[use system-wide target directory]
-z[supress most output]
--silent[supress most output]
